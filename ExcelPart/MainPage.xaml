<UserControl 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    
    xmlns:ExcelPart="clr-namespace:ExcelPart"
    xmlns:Controls="clr-namespace:excel_create.Controls" xmlns:Controls1="clr-namespace:ExcelPart.Controls" x:Class="ExcelPart.MainPage"
    mc:Ignorable="d" d:DesignWidth="400" Height="198">

    <Grid x:Name="LayoutRoot" Background="White" Margin="0,0,-107,-153">
        <StackPanel>

            <Controls1:PeopleChooser Name="SinglePeopleChooser" AllowMultiple="False" Width="Auto" d:LayoutOverrides="Height"/>
            <Controls1:PeopleChooser Name="MultiplePeopleChooser" AllowMultiple="True" Width="Auto" d:LayoutOverrides="Height" Loaded="MultiplePeopleChooser_Loaded"/>
            <Button Name="SubmitButton" Content="Submit" Click="SubmitButton_Click"/>

        </StackPanel>
    </Grid>
</UserControl>
